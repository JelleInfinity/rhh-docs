namespace GFLib.Model;

struct RGB {
  r:float;
  g:float;
  b:float;
}

struct RGBA {
  r:float;
  g:float;
  b:float;
  a:float;
}

struct RGBA8 {
  r:uint8;
  g:uint8;
  b:uint8;
  a:uint8;
}

struct Vec4 {
  x:float;
  y:float;
  z:float;
  w:float;
}

struct Vec3 {
  x:float;
  y:float;
  z:float;
}

struct Vec2 {
  x:float;
  y:float;
}

struct Box {
  b1:Vec3;
  b2:Vec3;
}

